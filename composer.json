{
    "name": "vinelab/neoeloquent",
    "description": "Laravel wrapper for the Neo4j graph database REST interface",
    "keywords": ["laravel", "neo4j", "graph", "database", "neoeloquent"],
    "license": "MIT",
    "authors": [
        {
            "name": "Abed Halawi",
            "email": "abed.halawi@vinelab.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "5.1.*",
        "illuminate/database": "5.1.*",
        "illuminate/pagination": "5.1.*",
        "heydavid713/neo4jphp": "0.1.*",
        "nesbot/carbon": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.2.*",
        "mockery/mockery": "0.9.*"
    },
    "repositories": [

    ],
    "autoload": {
        "classmap": [
            "tests/TestCase.php"
        ],
        "psr-0": {
            "Vinelab\\NeoEloquent\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "heydavid713/neo4jphp",
                "version": "0.1.1",
                "source": {
                    "url": "git://github.com/heydavid713/neo4jphp.git",
                    "type": "git",
                    "reference": "0.1.1"
                },
                "autoload": {
                    "psr-0": {
                        "Everyman\\neo4jphp\\": "lib/"
                    }
                }
            }
        }
    ]
}
